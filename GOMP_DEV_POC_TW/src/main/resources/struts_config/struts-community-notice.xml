<struts>
	<package name="notice" extends="dev-common" namespace="/community">
		<action name="listNotice" class="com.omp.dev.community.action.NoticeAction" method="listNotice">
			<result>/${loc.language}/jsp/community/notice/noticeList.jsp</result>
		</action>
		<action name="viewNotice" class="com.omp.dev.community.action.NoticeAction" method="viewNotice">
