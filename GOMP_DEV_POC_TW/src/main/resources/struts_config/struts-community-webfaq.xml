<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="webfaq" extends="dev-common" namespace="/community">
		<action name="iFrameListWebFaq" class="com.omp.dev.community.action.WebFaqAction" method="selectFaqList">
			<result name="success">/${loc.language}/jsp/community/faq/webFaqList.jsp</result>
		</action>
		<action name="iFrameDetailWebFaq" class="com.omp.dev.community.action.WebFaqAction" method="selectFaq">
			<result name="success">/${loc.language}/jsp/community/faq/webFaqDetail.jsp</result>
		</action>
	</package>
</struts>