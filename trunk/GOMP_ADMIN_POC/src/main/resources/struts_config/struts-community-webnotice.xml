<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="webnotice" extends="dev-common" namespace="/community">
		<action name="iFrameListWebNotice" class="com.omp.dev.community.action.WebNoticeAction" method="selectNoticeList">
			<result name="success">/${loc.language}/jsp/community/notice/webNoticeList.jsp</result>
		</action>
		<action name="iFrameDetailWebNotice" class="com.omp.dev.community.action.WebNoticeAction" method="selectNotice">
			<result name="success">/${loc.language}/jsp/community/notice/webNoticeDetail.jsp</result>
		</action>
		<action name="iFrameListWebSubNotice" class="com.omp.dev.community.action.WebNoticeAction" method="selectNoticeSubList">
			<result name="success">/${loc.language}/jsp/community/notice/webNoticeSubList.jsp</result>
		</action>
	</package>
</struts>